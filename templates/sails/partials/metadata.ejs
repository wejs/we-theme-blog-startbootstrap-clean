
<% if (metadata.description) { %>
  <meta property="description" content="<%- metadata.description %>">
<% } %>

<% if (metadata.keywords) { %>
  <meta name="keywords" content="<%= metadata.keywords %>">
<% } %>

<% if (metadata.application) { %>
  <% if (metadata.application.name) { %>
    <meta property="application-name" content="<%= metadata.application.name %>">
  <% } %>
  <% if (metadata.application.url) { %>
    <meta property="application-url" content="<%= metadata.application.url %>">
  <% } %>
<% } %>

<% if (metadata.og) { %>
  <% if (metadata.og.site_name) { %>
    <meta property="og:site_name" content="<%= metadata.og.site_name %>">
  <% } %>

  <% if (metadata.og.url) { %>
    <meta property="og:url" content="<%= metadata.og.url %>">
  <% } %>

  <% if (metadata.og.title) { %>
    <meta property="og:title" content="<%= metadata.og.title %>">
  <% } %>

  <% if (metadata.og.description) { %>
    <meta property="og:description" content="<%- metadata.og.description %>">
  <% } %>

  <% if (metadata.og.type) { %>
    <meta property="og:type" content="<%= metadata.og.type %>">
  <% } %>

  <% if (metadata.og.image) { %>
    <meta property="og:image" content="<%= metadata.og.image %>">
  <% } %>
<% } %>
